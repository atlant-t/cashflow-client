{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "yarn",
    "analytics": "3df01c4f-c0a2-40d1-9365-2ba2287cab98"
  },
  "schematics": {
    "@schematics/angular:component": {
      "style": "scss"
    }
  },
  "newProjectRoot": "packages",
  "projects": {
    "app": {
      "projectType": "application",
      "root": "packages/app",
      "sourceRoot": "packages/app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/app",
            "index": "packages/app/src/index.html",
            "main": "packages/app/src/main.ts",
            "polyfills": "packages/app/src/polyfills.ts",
            "tsConfig": "packages/app/tsconfig.app.json",
            "aot": true,
            "assets": [
              "packages/app/src/assets"
            ],
            "styles": [
              "packages/app/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "environments/app.ts",
                  "with": "environments/app.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "packages/app/test.ts",
            "polyfills": "packages/app/src/polyfills.ts",
            "tsConfig": "packages/app/tsconfig.spec.json",
            "karmaConfig": "packages/app/karma.conf.js",
            "assets": [
              "packages/app/src/favicon.ico",
              "packages/app/src/assets"
            ],
            "styles": [
              "packages/app/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "packages/app/tsconfig.app.json",
              "packages/app/tsconfig.spec.json",
              "packages/app/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "packages/app/e2e/protractor.conf.js",
            "devServerTarget": "app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "app:serve:production"
            }
          }
        }
      }
    }},
  "defaultProject": "app"
}
